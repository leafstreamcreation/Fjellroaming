# DNS Provider Configuration
DNSP_ENDPOINT=https://api.dnsprovider.com/v1
DNSP_API_KEY=your-dns-provider-api-key
DNSP_API_SECRET=your-dns-provider-secret-key

# DDNS Provider Configuration (for IP detection)
DDNSP_ENDPOINT=https://api.dnsprovider.com/v1/getip
DDNSP_API_KEY=your-ddns-provider-api-key

# Domain Configuration
DOMAIN=yourdomain.com
SUBDOMAINS=www,api,mail

# Check interval in milliseconds (default: 600000 = 10 minutes)
CHECK_INTERVAL=600000
